{
    "name": "@svelte-web-fonts/google",
    "description": "Load fonts via google with typescript auto-completion",
    "version": "1.0.1",
    "license": "MIT",
    "svelte": "dist/index.js",
    "module": "dist/index.mjs",
    "main": "dist/index.umd.js",
    "types": "dist/ts/index.d.ts",
    "author": {
        "email": "contact@jakob-kruse.de",
        "name": "Jakob Kruse",
        "url": "https://github.com/jakob-kruse"
    },
    "homepage": "https://github.com/svelte-web-fonts/google#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/svelte-web-fonts/google"
    },
    "bugs": {
        "url": "https://github.com/svelte-web-fonts/google/issues",
        "email": "svelte-web-fonts-google@hostname.com"
    },
    "scripts": {
        "prebuild": "rm -rf ./dist",
        "build": "rollup -c",
        "build:watch": "rollup -c -w",
        "format": "prettier --config .prettierrc.json src/**/*.{ts,svelte} --write && npm run lint:fix",
        "lint": "svelte-check --ignore dist && eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prepublishOnly": "npm run build && npm run format && npm run test",
        "generate": "ts-node -r dotenv/config src/generator/index.ts && npm run format",
        "prepare": "husky install",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-typescript": "^6.0.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/svelte": "^3.0.3",
        "@tsconfig/svelte": "^1.0.10",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.6.1",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "fs-extra": "^9.0.1",
        "glob": "^7.1.6",
        "husky": "^7.0.0",
        "prettier": "^2.3.2",
        "prettier-plugin-svelte": "^2.3.1",
        "rollup": "^2.32.0",
        "rollup-plugin-execute": "^1.1.1",
        "rollup-plugin-filesize": "^9.0.2",
        "rollup-plugin-svelte": "^6.0.1",
        "rollup-plugin-terser": "^7.0.2",
        "svelte": "^3.24.0",
        "svelte-check": "^1.0.60",
        "svelte-preprocess": "^4.5.1",
        "ts-node": "^10.2.0",
        "tslib": "^2.0.3",
        "typescript": "^4.3.5",
        "jest": "^27.0.6",
        "ts-jest": "^27.0.4"
    },
    "keywords": [
        "svelte",
        "fonts"
    ]
}
